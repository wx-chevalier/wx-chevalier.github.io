&lt;/span>&lt;/span>&lt;span class="line">&lt;span class="cl"> &lt;span class="k">if&lt;/span> &lt;span class="p">(&lt;/span>&lt;span class="nx">axios&lt;/span>&lt;span class="p">.&lt;/span>&lt;span class="nx">isCancel&lt;/span>&lt;span class="p">(&lt;/span>&lt;span class="nx">thrown&lt;/span>&lt;span class="p">))&lt;/span> &lt;span class="p">{&lt;/span>
